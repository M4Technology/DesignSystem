{
  "name": "professordev-designsystem",
  "version": "0.1.6",
  "private": false,
  "dependencies": {
    "@bit/ironmanvim.comp.fontawesome": "0.0.1",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "classnames": "^2.2.6",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "3.4.1",
    "prop-types": "latest"
  },
  "scripts": {
    "start": "npm run watch:css && react-scripts start",
    "build": "cross-env BABEL_ENV=production babel src -d dist",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "watch:css": "postcss src/assets/css/*.css --dir src/assets/css/build --watch &",
    "build:css": "postcss src/assets/css/*.css --dir src/assets/css/build",
    "storybook": "npm run watch:css && start-storybook -p 9009 -s public",
    "build-storybook": "npm run build:css && build-storybook -s public",
    "release": "auto shipit"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.10.4",
    "@storybook/addon-actions": "^5.3.19",
    "@storybook/addon-docs": "^5.3.19",
    "@storybook/addon-knobs": "^5.3.19",
    "@storybook/addon-links": "^5.3.19",
    "@storybook/addon-storysource": "^5.3.19",
    "@storybook/addon-viewport": "^5.3.19",
    "@storybook/addons": "^5.3.19",
    "@storybook/client-api": "latest",
    "@storybook/preset-create-react-app": "^3.0.0",
    "@storybook/react": "^5.3.19",
    "auto": "^9.43.1",
    "autoprefixer": "^9.8.0",
    "cross-env": "^7.0.2",
    "postcss-cli": "^7.1.1",
    "tailwindcss": "^1.4.6"
  },
  "babel": {
    "presets": [
      [
        "react-app",
        {
          "absoluteRuntime": false
        }
      ]
    ]
  },
  "description": "This Design System is bootstrapped with [StoryBook](https://storybook.js.org/) and [TailwindCSS](https://tailwindcss.com/).",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ProfessorDev/DesignSystem.git"
  },
  "keywords": [
    "Design",
    "System",
    "DesignSystem",
    "ProfessorDev",
    "Professor"
  ],
  "author": "Vishal Chiluveri",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ProfessorDev/DesignSystem/issues"
  },
  "homepage": "https://github.com/ProfessorDev/DesignSystem#readme"
}
